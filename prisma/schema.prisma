generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}


model Cafe {
  id      String   @id @default(auto()) @map("_id") @db.ObjectId
  listId String @unique
  name String
  description String?
  img String
  address String
  mycomment String?
  instagram String?
  keywords String[]

}